{
    "version": 2,
    "builds": [
        {
            "src": "loopin-backend/api/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "loopin-backend/api/index.js"
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/loopin-backend/api/index.js"
        }
    ]
}