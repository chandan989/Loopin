{
    "version": 2,
    "buildCommand": "cd loopin-web && npm install && npm run build",
    "outputDirectory": "loopin-web/dist",
    "installCommand": "npm install --prefix loopin-web",
    "framework": null,
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}